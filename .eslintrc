{
  // "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/recommended",
  ],
  "env": {
    "webextensions": true,
    "browser": true,
    "es6": true,
  },
  "rules": {
    "max-len": [2, 120, 2],
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js", 
          ".jsx", 
          ".ts", 
          ".tsx",
        ],
      },
    },
  },
}