{
  "compilerOptions": {
    "baseUrl": "./src",
    "esModuleInterop": true,
    "lib": [
      "dom",
      "es6"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "dist",
    "sourceMap": true,
    "target": "es6",
  },
  "exclude": [
    "node_modules/"
  ]
}